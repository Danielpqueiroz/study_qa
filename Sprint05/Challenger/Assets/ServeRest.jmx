              <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy
import org.apache.jmeter.protocol.http.control.HeaderManager
import org.apache.jmeter.protocol.http.control.Header
import org.apache.jmeter.protocol.http.util.HTTPConstants

// Recupera a variável userId
def userId = vars.get(&apos;userId&apos;)

if (userId != null) {
    // Configuração do HTTP Sampler para apagar o usuário
    def httpSampler = new HTTPSamplerProxy()
    httpSampler.setDomain(&apos;localhost&apos;)
    httpSampler.setPort(3000)
    httpSampler.setPath(&quot;/usuarios/${userId}&quot;)
    httpSampler.setMethod(HTTPConstants.DELETE)
    httpSampler.setProtocol(&apos;http&apos;)

    // Configuração do Header Manager
    def headerManager = new HeaderManager()
    headerManager.add(new Header(&apos;Content-Type&apos;, &apos;application/json&apos;))
    httpSampler.setHeaderManager(headerManager)

    // Executa a requisição
    def result = httpSampler.sample()

    // Verifica a resposta
    if (result.getResponseCode() == &apos;200&apos;) {
        log.info(&apos;Usuário apagado com sucesso: &apos; + userId)
    } else {
        log.error(&apos;Erro ao apagar usuário: &apos; + result.getResponseCode() + &apos; - &apos; + result.getResponseMessage())
    }
} else {
    log.error(&apos;Variável userId não encontrada.&apos;)
