              <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy
import org.apache.jmeter.protocol.http.control.HeaderManager
import org.apache.jmeter.protocol.http.util.HTTPConstants
import org.apache.jmeter.samplers.SampleResult
import groovy.json.JsonSlurper
import java.util.UUID

// Gera um e-mail aleatório
String randomEmail = &quot;user_&quot; + UUID.randomUUID().toString() + &quot;@example.com&quot;

// Configuração da requisição POST
HTTPSamplerProxy postSampler = new HTTPSamplerProxy()
postSampler.setDomain(&quot;localhost&quot;)
postSampler.setPort(3000)
postSampler.setPath(&quot;/usuarios&quot;)
postSampler.setMethod(HTTPConstants.POST)
postSampler.addNonEncodedArgument(&quot;&quot;, 
    &apos;{&quot;nome&quot;:&quot;Usuário Teste&quot;,&quot;email&quot;:&quot;&apos; + randomEmail + &apos;&quot;,&quot;password&quot;:&quot;senhateste&quot;,&quot;administrador&quot;:&quot;true&quot;}&apos;, 
    &quot;&quot;)

// Criar o HeaderManager
HeaderManager headerManager = new HeaderManager()
postSampler.setHeaderManager(headerManager)

// Adicionar o cabeçalho Content-Type
headerManager.add(new org.apache.jmeter.protocol.http.control.Header(&quot;Content-Type&quot;, &quot;application/json&quot;))

// Executa a requisição POST
SampleResult postResult = postSampler.sample()

// Verifica se a requisição foi bem-sucedida
if (postResult.getResponseCode().equals(&quot;201&quot;)) {
    def jsonResponse = new JsonSlurper().parseText(postResult.getResponseDataAsString())
    String createdUserId = jsonResponse._id.toString()
    
    // Log para depuração
    log.info(&quot;createdUserId: &quot; + createdUserId)
    log.info(&quot;randomEmail: &quot; + randomEmail)
    
    vars.put(&quot;createdUserId&quot;, createdUserId)
    vars.put(&quot;randomEmail&quot;, randomEmail)
} else {
    throw new Exception(&quot;Failed to create user: &quot; + postResult.getResponseMessage())
}</stringProp>
